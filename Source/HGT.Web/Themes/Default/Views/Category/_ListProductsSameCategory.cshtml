@model IEnumerable<ProductViewModel>
@using HGT.Web.Models;

@if (Model!= null && Model.Count() > 0)
{
    <h2>Sản phẩm khác</h2>
    <div id="owl1">
        @foreach (var item in Model)
        {
            <div class="item1">
                 @Html.Partial("_ProductBox", item)
            </div>
        }
    </div>
}